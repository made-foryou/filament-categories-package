{
    "name": "made-foryou/filament-categories",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MadeForYou\\Categories\\": "src/",
            "MadeForYou\\Categories\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadeForYou\\Categories\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Menno Tempelaar",
            "email": "menno@made-foryou.nl",
            "role": "dev"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./../filament-helpers"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~8.2",
        "spatie/laravel-package-tools": "^1.16",
        "filament/filament": "^3.1",
        "livewire/livewire": "^3.2",
        "made-foryou/filament-helpers": "dev-main"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "pestphp/pest": "^2.28",
        "orchestra/testbench": "^8.17",
        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeForYou\\Categories\\FilamentCategoriesServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
